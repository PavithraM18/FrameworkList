package practiceCode;

public class ReverseArray {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] a = {1,2,3,4,5,6,7,8,9,0};
		int rev = 0 ;
		int[] val = {};
		for(int i = a.length-1; i < 0  ; i--)
		{
			rev = a[i];
			val = rev + val;
		}
		System.out.println(val);
	}

}