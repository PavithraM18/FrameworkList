package Programs;

import java.util.ArrayList;
import java.util.Arrays;

public class ContainsDuplicate {
	
	public boolean containsDuplicate(int[] num) {
		int[] nums = [1,2,3,1] ;
	Arrays.sort(nums);
		for (int i = 1; i < nums.length; i++) {
	        if (nums[i] == nums[i - 1]) {
	            return true;
	        }
	    }
	    return false;   

	}
}


