		int[] nums = {2,7,11,15};
		int target = 9;
		twoSum(nums, target);
		System.out.println(nums);
	}

	 public static int[] twoSum(int[] nums, int target) {
	      int[] ans = new int[];
		 for (int i = 0;i<nums.length; i++)
		 {
			 for (int j= i+1;j<nums.length;j++)
			 {
				 if (target == nums[i]+nums[j])
				 {
					 
				 }
		 }
	    }
		return nums;
}
}
